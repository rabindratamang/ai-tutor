{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "FastAPI",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": ["main:app", "--reload"],
            "cwd": "${workspaceFolder}/backend",
            "jinja": true,
            "justMyCode": true,
            "python": "${workspaceFolder}/backend/.venv/bin/python",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/backend",
                "VIRTUAL_ENV": "${workspaceFolder}/backend/.venv"
            }
        }
    ]
}